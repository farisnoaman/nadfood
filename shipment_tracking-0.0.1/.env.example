# Supabase Configuration
# Copy this file to .env and replace with your actual credentials

VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Real-time Features Control
# Set to 'false' to disable realtime subscriptions and prevent API key exposure
VITE_ENABLE_REALTIME=true

# Note: SUPABASE_SERVICE_ROLE_KEY should NEVER be in .env (client-side)
# It must only be set in Supabase Edge Functions using: supabase secrets set
